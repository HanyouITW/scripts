#!/bin/bash

#PBS -S /bin/bash
#PBS -A ACF-UTHSC0013
#PBS -l nodes=1:ppn=40,walltime=140:00:00
#PBS -m abe
#PBS -M hanyou@umich.edu
#PBS -j oe
#PBS -l partition=general,feature=skylake
#PBS -N 2019-10-02_x_long_ranger_wgs_95-39694.qsub


source /nics/c/home/hanyou/.bashrc
cd

mkdir -p /lustre/haven/proj/UTHSC0013/UM/projects/rat_genome_03/long_ranger/95-39694
cd /lustre/haven/proj/UTHSC0013/UM/projects/rat_genome_03/long_ranger/95-39694

longranger wgs --id=95-39694 --reference=/lustre/haven/proj/UTHSC0013/UM/projects/rat_genome_03/references/refdata-rn6.fa.concatenate --fastqs=/lustre/haven/proj/UTHSC0013/UM/projects/rat_genome_03/sequences/95-39694/ --sample=95-39694_1,95-39694_2,95-39694_3,95-39694_4 --vcmode=gatk:/lustre/haven/proj/UTHSC0013/UM/apps/gatk_4.0.3.0/gatk-4.0.3.0/gatk-package-4.0.3.0-local.jar

